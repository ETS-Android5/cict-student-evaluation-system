<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="app.sms.models.SmsQueryMapping" table="sms_query" catalog="sms_server" dynamic-update="true" dynamic-insert="true">
<id name="id" type="java.lang.Integer">
<column name="id"/>
<generator class="identity"/>
</id>
<property name="reciepients_number" type="java.lang.String">
<column name="reciepients_number" length="50"/>
</property>
<property name="message_body" type="java.lang.String">
<column name="message_body" length="160"/>
</property>
<property name="sender_name" type="java.lang.String">
<column name="sender_name" length="50"/>
</property>
<property name="date" type="java.util.Date">
<column name="date" length="19"/>
</property>
<property name="status" type="java.lang.Integer">
<column name="status" length="10"/>
</property>
<property name="active" type="java.lang.Integer">
<column name="active" length="10"/>
</property>
</class>
</hibernate-mapping>
