<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="app.lazy.models.SubjectMapping" table="subject" catalog="cictems" dynamic-update="true" dynamic-insert="true">
<id name="id" type="java.lang.Integer">
<column name="id"/>
<generator class="identity"/>
</id>
<property name="code" type="java.lang.String">
<column name="code" length="100"/>
</property>
<property name="descriptive_title" type="java.lang.String">
<column name="descriptive_title" length="100"/>
</property>
<property name="lec_units" type="java.lang.Double">
<column name="lec_units" length="22"/>
</property>
<property name="lab_units" type="java.lang.Double">
<column name="lab_units" length="22"/>
</property>
<property name="type" type="java.lang.String">
<column name="type" length="50"/>
</property>
<property name="subtype" type="java.lang.String">
<column name="subtype" length="50"/>
</property>
<property name="added_by" type="java.lang.Integer">
<column name="added_by" length="10"/>
</property>
<property name="added_date" type="java.util.Date">
<column name="added_date" length="19"/>
</property>
<property name="updated_by" type="java.lang.Integer">
<column name="updated_by" length="10"/>
</property>
<property name="updated_date" type="java.util.Date">
<column name="updated_date" length="19"/>
</property>
<property name="active" type="java.lang.Integer">
<column name="active" length="10"/>
</property>
</class>
</hibernate-mapping>
