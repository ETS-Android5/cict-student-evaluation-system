<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="app.lazy.models.FacultyDataHistoryMapping" table="faculty_data_history" catalog="cictems" dynamic-update="true" dynamic-insert="true">
<id name="id" type="java.lang.Integer">
<column name="id"/>
<generator class="identity"/>
</id>
<property name="faculty_id" type="java.lang.Integer">
<column name="faculty_id" length="10"/>
</property>
<property name="bulsu_id" type="java.lang.String">
<column name="bulsu_id" length="50"/>
</property>
<property name="last_name" type="java.lang.String">
<column name="last_name" length="50"/>
</property>
<property name="first_name" type="java.lang.String">
<column name="first_name" length="50"/>
</property>
<property name="middle_name" type="java.lang.String">
<column name="middle_name" length="50"/>
</property>
<property name="gender" type="java.lang.String">
<column name="gender" length="50"/>
</property>
<property name="contact_number" type="java.lang.String">
<column name="contact_number" length="50"/>
</property>
<property name="rank" type="java.lang.String">
<column name="rank" length="50"/>
</property>
<property name="department" type="java.lang.String">
<column name="department" length="50"/>
</property>
<property name="updated_by" type="java.lang.Integer">
<column name="updated_by" length="10"/>
</property>
<property name="updated_date" type="java.util.Date">
<column name="updated_date" length="19"/>
</property>
<property name="active" type="java.lang.Integer">
<column name="active" length="3"/>
</property>
</class>
</hibernate-mapping>
