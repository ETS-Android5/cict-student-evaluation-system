<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="app.lazy.models.SystemVariablesMapping" table="system_variables" catalog="cictems" dynamic-update="true" dynamic-insert="true">
<id name="id" type="java.lang.Integer">
<column name="id"/>
<generator class="identity"/>
</id>
<property name="name" type="java.lang.String">
<column name="name" length="50"/>
</property>
<property name="value" type="java.lang.String">
<column name="value" length="300"/>
</property>
<property name="created_by" type="java.lang.Integer">
<column name="created_by" length="10"/>
</property>
<property name="created_date" type="java.util.Date">
<column name="created_date" length="19"/>
</property>
<property name="updated_by" type="java.lang.Integer">
<column name="updated_by" length="10"/>
</property>
<property name="updated_date" type="java.util.Date">
<column name="updated_date" length="19"/>
</property>
<property name="active" type="java.lang.Integer">
<column name="active" length="10"/>
</property>
</class>
</hibernate-mapping>
