<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="app.lazy.models.CurriculumHistorySummaryMapping" table="curriculum_history_summary" catalog="cictems" dynamic-update="true" dynamic-insert="true">
<id name="history_id" type="java.lang.Integer">
<column name="history_id"/>
<generator class="identity"/>
</id>
<property name="curriculum_id" type="java.lang.Integer">
<column name="curriculum_id" length="10"/>
</property>
<property name="description" type="java.lang.String">
<column name="description" length="300"/>
</property>
<property name="created_by" type="java.lang.Integer">
<column name="created_by" length="10"/>
</property>
<property name="created_date" type="java.util.Date">
<column name="created_date" length="19"/>
</property>
<property name="active" type="java.lang.Integer">
<column name="active" length="3"/>
</property>
</class>
</hibernate-mapping>
