<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="app.lazy.models.AcademicProgramMapping" table="academic_program" catalog="cictems" dynamic-update="true" dynamic-insert="true">
<id name="id" type="java.lang.Integer">
<column name="id"/>
<generator class="identity"/>
</id>
<property name="code" type="java.lang.String">
<column name="code" length="50"/>
</property>
<property name="name" type="java.lang.String">
<column name="name" length="50"/>
</property>
<property name="floor_assignment" type="java.lang.Integer">
<column name="floor_assignment" length="10"/>
</property>
<property name="created_date" type="java.util.Date">
<column name="created_date" length="19"/>
</property>
<property name="created_by" type="java.lang.Integer">
<column name="created_by" length="10"/>
</property>
<property name="removed_date" type="java.util.Date">
<column name="removed_date" length="19"/>
</property>
<property name="removed_by" type="java.lang.Integer">
<column name="removed_by" length="10"/>
</property>
<property name="implemented" type="java.lang.Integer">
<column name="implemented" length="3"/>
</property>
<property name="implementation_date" type="java.util.Date">
<column name="implementation_date" length="19"/>
</property>
<property name="implemented_by" type="java.lang.Integer">
<column name="implemented_by" length="10"/>
</property>
<property name="active" type="java.lang.Integer">
<column name="active" length="10"/>
</property>
</class>
</hibernate-mapping>
