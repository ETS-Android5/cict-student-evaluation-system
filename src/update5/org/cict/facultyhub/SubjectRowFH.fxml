<HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" prefHeight="55.0" prefWidth="809.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@metro-ui.css" />
