<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="application_root" prefHeight="600.0" prefWidth="500.0" style="-fx-background-color: #FF3675;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER_LEFT" prefHeight="41.0" prefWidth="291.0">
         <children>
            <Label styleClass="metro-title-33" text="System Values" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </VBox>
      <StackPane VBox.vgrow="ALWAYS">
         <children>
            <VBox fx:id="vbox_preview">
               <children>
                  <Label text="Any changes made within the following information will not reflect to the previous transactions, hence it will only affect the procedures following after the alteration." wrapText="true">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label text="Click the extension button to show the value.">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin></Label>
                  <VBox fx:id="vbox_table_holder" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="176.0" prefWidth="438.0" styleClass="metro-table" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </VBox>
                  <VBox alignment="CENTER_RIGHT" prefHeight="39.0" prefWidth="525.0">
                     <children>
                        <HBox alignment="CENTER_RIGHT" prefHeight="54.0" prefWidth="525.0" spacing="15.0">
                           <children>
                              <JFXButton fx:id="btn_new_field" prefHeight="48.0" prefWidth="132.0" style="-fx-border-color: #fff;" text="New Field" textFill="WHITE" visible="false">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </JFXButton>
                              <JFXButton fx:id="btn_cancel" prefHeight="48.0" prefWidth="132.0" style="-fx-border-color: #fff;" text="Cancel" textFill="WHITE">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </JFXButton>
                           </children>
                        </HBox>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </VBox>
               </children>
               <StackPane.margin>
                  <Insets />
               </StackPane.margin>
            </VBox>
            <VBox fx:id="vbox_new" prefHeight="104.0" prefWidth="457.0" visible="false">
               <children>
                  <VBox prefHeight="175.0" prefWidth="475.0" spacing="10.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="Field Name">
                           <font>
                              <Font name="System Bold" size="13.0" />
                           </font>
                        </Label>
                        <TextField fx:id="txt_name" prefHeight="33.0" prefWidth="666.0">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </TextField>
                        <Label layoutX="10.0" layoutY="10.0" text="Field Value">
                           <font>
                              <Font name="System Bold" size="13.0" />
                           </font>
                        </Label>
                        <TextField fx:id="txt_value" layoutX="10.0" layoutY="29.0" prefHeight="33.0" prefWidth="666.0" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" right="10.0" />
                     </VBox.margin>
                  </VBox>
                  <VBox alignment="CENTER_RIGHT" prefHeight="39.0" prefWidth="485.0">
                     <children>
                        <HBox alignment="CENTER_RIGHT" prefHeight="74.0" prefWidth="485.0" spacing="15.0">
                           <children>
                              <JFXButton fx:id="btn_add_new" prefHeight="48.0" prefWidth="132.0" style="-fx-border-color: #fff;" text="Add New" textFill="WHITE">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </JFXButton>
                              <JFXButton fx:id="btn_cancel1" prefHeight="42.0" prefWidth="132.0" style="-fx-border-color: #fff;" text="Back" textFill="WHITE">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </JFXButton>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
               <StackPane.margin>
                  <Insets left="20.0" right="20.0" top="10.0" />
               </StackPane.margin>
            </VBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="15.0" right="15.0" />
         </VBox.margin>
      </StackPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <stylesheets>
      <URL value="@metro-ui.css" />
      <URL value="@metro-ui-table.css" />
   </stylesheets>
</VBox>
