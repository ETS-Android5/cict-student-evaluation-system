<HBox fx:id="application_root" maxWidth="1.7976931348623157E308" prefHeight="86.0" prefWidth="810.0" styleClass="metro-row" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
