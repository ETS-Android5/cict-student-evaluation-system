<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="app.lazy.models.GradeMapping" table="grade" catalog="cictems" dynamic-update="true" dynamic-insert="true">
<id name="id" type="java.lang.Integer">
<column name="id"/>
<generator class="identity"/>
</id>
<property name="STUDENT_id" type="java.lang.Integer">
<column name="STUDENT_id" length="10"/>
</property>
<property name="SUBJECT_id" type="java.lang.Integer">
<column name="SUBJECT_id" length="10"/>
</property>
<property name="ACADTERM_id" type="java.lang.Integer">
<column name="ACADTERM_id" length="10"/>
</property>
<property name="rating" type="java.lang.String">
<column name="rating" length="50"/>
</property>
<property name="remarks" type="java.lang.String">
<column name="remarks" length="100"/>
</property>
<property name="credit" type="java.lang.Double">
<column name="credit" length="22"/>
</property>
<property name="credit_method" type="java.lang.String">
<column name="credit_method" length="100"/>
</property>
<property name="created_by" type="java.lang.Integer">
<column name="created_by" length="10"/>
</property>
<property name="created_date" type="java.util.Date">
<column name="created_date" length="19"/>
</property>
<property name="posted" type="java.lang.Integer">
<column name="posted" length="10"/>
</property>
<property name="posted_by" type="java.lang.Integer">
<column name="posted_by" length="10"/>
</property>
<property name="posting_date" type="java.util.Date">
<column name="posting_date" length="19"/>
</property>
<property name="inc_expire" type="java.util.Date">
<column name="inc_expire" length="19"/>
</property>
<property name="updated_by" type="java.lang.Integer">
<column name="updated_by" length="10"/>
</property>
<property name="updated_date" type="java.util.Date">
<column name="updated_date" length="26"/>
</property>
<property name="reason_for_update" type="java.lang.String">
<column name="reason_for_update" length="50"/>
</property>
<property name="referrence_curriculum" type="java.lang.Integer">
<column name="referrence_curriculum" length="10"/>
</property>
<property name="active" type="java.lang.Integer">
<column name="active" length="10"/>
</property>
</class>
</hibernate-mapping>
